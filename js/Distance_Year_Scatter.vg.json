{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "height": 500,
  "width": 500,
  "title": "Distance and Establishments",
  "data": {
    "url": "https://raw.githubusercontent.com/KEarlsKebab/3179/refs/heads/main/data/VicNatParks.csv"
  },
  "mark": {"type":"circle"},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "quantitative",
      "title":"Year Established",
      "axis": {"tickCount": 7},
      "scale": {"domain":[1890, 2020]}
    },
    "y": {
      "field": "Melbourne_Distance",
      "type": "quantitative",
      "axis": { "tickCount": 6},
      "title":"Distance From Melbournce (km)",
      "scale": {"domain": [0,500]}
    },
    "size": {"field": "Area (Ha)", "type": "quantitative", "scale": {"type": "threshold",
"domain": [1000, 10000, 100000, 300000, 600000],
"range": [50, 100, 200, 500, 750, 1000]},
"legend": {"format": ".1s"}},
    "color": {"field": "Type", "type": "nominal"},
  "tooltip":[{"field":"Name"},{"field":"Area (Ha)"},
  {"field":"Melbourne_Distance", "title":"Distance to CBD (km)"},
  {"field":"Year"}]
  }
}
