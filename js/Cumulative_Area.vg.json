{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": { "text": "Area over Time" },
    "data": {
        "url": "https://raw.githubusercontent.com/KEarlsKebab/3179/refs/heads/main/data/CumulativeArea.csv"
    },
    "mark": "bar",
    "encoding": {
        "x": {"timeUnit": "year", "field": "Year", "type": "ordinal"},
        "y": { "aggregate": "sum", "field": "Area_Ha", "title": "Total Area (Ha)"
        }
    }
}
