{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 600,
  "height": 450,
  "title": "Victorian National Parks",
  "projection": {"type": "equirectangular",
    "center": [145.5, -37],
    "scale": 3400},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/KEarlsKebab/3179/refs/heads/main/data/victoria.json",
        "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "black"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/KEarlsKebab/3179/refs/heads/main/js/output.json",
        "format": {"type": "topojson", "feature":"Campgrounds"}
      },
      "transform": [
    {
      "filter": "slice(datum.properties.PARK_NAME, length(datum.properties.PARK_NAME) - 13) == 'National Park'"
    }
  ],
      "mark": {"type": "geoshape", "fill": "black", "stroke": "white"},
      "encoding": {
        "fill": {"field":"properties.PARK_NAME"},
      "tooltip": [{"field": "properties.PARK_NAME", "type": "nominal", "title": "Park"},
    {"field": "properties.SITE_NAME", "type": "nominal", "title": "Site"}]}
    }
  ]
}
